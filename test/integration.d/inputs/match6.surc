seven: Integer = 7

weird(x: Integer) = match x
	4 -> 6
	5 -> 2
	n if (n == 2) -> 1
	n if (n > seven) -> 9
	n -> n

main(stdout: OutputStream) = do
	write stdout , 0 >> weird >> str
	write stdout , 1 >> weird >> str
	write stdout , 2 >> weird >> str
	write stdout , 3 >> weird >> str
	write stdout , 4 >> weird >> str
	write stdout , 5 >> weird >> str
	write stdout , 6 >> weird >> str
	write stdout , 7 >> weird >> str
	write stdout , 8 >> weird >> str
	write stdout , 9 >> weird >> str
